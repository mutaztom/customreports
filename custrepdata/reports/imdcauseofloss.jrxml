             VGS_INV_CUR_RAT,
             KEY_ITM_NBR,
            VIT_WGT_NET,
            VIT_CIF ,  
            VIT_INV_AMT_FCX ,
            VIT_INV_CUR_RAT , 
            TAR_HSC_NB1 ,
            TAR_HSC_NB2,
            TAR_PRC_EXT,
            TAR_PRC_NAT,
            TAR_PRF
from (      select
            gen_seg.INSTANCEID INSTANCEID,
decode (gen_seg.VGS_INV_CUR_RAT,gen_seg.VGS_INV_CUR_RAT,(select 
                            AWCURUPDATE_ITM.CUR_NRAT  CUR_NRAT
                            from    
                             AWCURUPDATE_ITM,
                             AWCURUPDATE_GEN
