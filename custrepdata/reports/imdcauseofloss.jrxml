            SAD_ITEM.VIT_CIF VIT_CIF,  
            SAD_ITEM.VIT_INV_AMT_FCX VIT_INV_AMT_FCX,
            SAD_ITEM.VIT_INV_CUR_RAT VIT_INV_CUR_RAT, 
