	<queryString language="SQL">
		<![CDATA[SELECT DISTINCT "CommRates" "Rates", SUM( "Weight") as "Net Nass (kg)", SUM( "LocalSadTax") as "Customs Value (SDG)", SUM(DISTINCT USD) as "Customs Value (USD)", SUM( "Duties - Base by TaxRate") as "Duty (SDG)", SUM( "Collected") as "Duty Colllected (SDG)", SUM( "Losses" )as "Duty Loss"
FROM(
  
  SELECT 
       UNTARTAB.TAR_T01 AS "CommRates",
       sum(SAD_ITEM.VIT_WGT_NET) AS "Weight",
       sum(SAD_TAX.TAX_LIN_BSE) AS "LocalSadTax",
       sum((SAD_ITEM.VIT_INV_AMT_FCX*SAD_GENERAL_SEGMENT.VGS_INV_CUR_RAT)/18) AS "USD", 
       sum(SAD_TAX.TAX_LIN_BSE * SAD_TAX.TAX_LIN_RAT) AS "Duties - Base by TaxRate",
       sum(SAD_TAX.TAX_LIN_AMT)/100 AS "Collected",
       sum(SAD_RELIEF.TAX_AMOUNT)/100 AS "Losses",SAD_ITEM.TAR_HSC_NB1,
       TRIM(LEADING  '0' from TO_CHAR(SAD_GENERAL_SEGMENT.IDE_RCP_DAT, 'mm-yyyy')) AS ONDATE
 FROM
       SAD_ITEM,
       SAD_TAX,
       SAD_GENERAL_SEGMENT,
       SAD_RELIEF,
       UNTARTAB
 Where
       SAD_GENERAL_SEGMENT.INSTANCEID = SAD_ITEM.INSTANCEID and 
       SAD_GENERAL_SEGMENT.IDE_REG_SER in ('W','L','A','P','C','G','K') and
       SAD_GENERAL_SEGMENT.IDE_TYP_PRC ='4'and
       SAD_ITEM.INSTANCEID = SAD_TAX.INSTANCEID and
       SAD_ITEM.KEY_ITM_NBR =  SAD_TAX.KEY_ITM_NBR and
       substr(SAD_ITEM.TAR_HSC_NB1,1,4)  NOT IN ('1701','1001','0902') and 
       SAD_TAX.INSTANCEID = SAD_RELIEF.INSTANCEID and
       SAD_TAX.KEY_ITM_NBR =  SAD_RELIEF.KEY_ITM_NBR and
       SAD_TAX.TAX_LIN_COD =  SAD_RELIEF.TAX_CODE and
       SAD_TAX.TAX_LIN_COD = 'IMD' and 
       TRIM(LEADING '0' from TO_CHAR(SAD_GENERAL_SEGMENT.IDE_RCP_DAT, 'yyyy')) =$P{year} and 

       concat (UNTARTAB.HS6_COD,UNTARTAB.TAR_PR1) = SAD_ITEM.TAR_HSC_NB1 and
       UNTARTAB.VALID_TO is null
 Group BY
       UNTARTAB.TAR_T01,SAD_ITEM.TAR_HSC_NB1,
       TRIM(LEADING '0' from TO_CHAR(SAD_GENERAL_SEGMENT.IDE_RCP_DAT, 'mm-yyyy'))
) Group by "CommRates"]]>
	</queryString>
